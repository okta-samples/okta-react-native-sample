    "@babel/runtime": "^7.24.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.8",
    "enzyme-async-helpers": "0.9.1",
    "eslint": "^9.0.0",
    "eslint-plugin-jest": "^26.8.7",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react-dom": "18.2.0",
